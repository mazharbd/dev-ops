pipeline{
	agent any
	stages{
	 stage('Build Application'){
	 steps{
	    echo AnyPoint_Environment
	 	git 'https://github.com/mazharbd/test-ci-cd.git'
	 	sh 'mvn clean install'
	 }
	 }
	 
	 stage('Deploy Application'){
	 steps{
	    

        // Run Maven on a Unix agent.
        sh 'mvn package deploy -DmuleDeploy'
	 }
	 }
	}
}