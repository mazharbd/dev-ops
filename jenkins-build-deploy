pipeline{
	agent any
	stages{
	 stage('Build Application'){
	 steps{
	 bat 'mvn clean install'
	 }
	 }
	 
	 stage('Deploy Application'){
	 steps{
	    git 'https://github.com/mazharbd/test-ci-cd.git'

            // Run Maven on a Unix agent.
        sh "mvn -Dmaven.test.failure.ignore=true clean package"
	 }
	 }
	}
}